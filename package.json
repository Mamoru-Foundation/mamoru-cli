{
    "name": "mamoru-cli",
    "version": "0.0.1",
    "main": "dist/index.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Mamoru-Foundation/mamoru-cli.git"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "lint": "eslint ./src --fix",
        "lint:ci": "eslint ./src",
        "format:ci": "prettier -c ./src/**/*.ts",
        "format": "prettier --write -c ./src/**/*.ts",
        "prepare": "husky install",
        "dev": "ts-node ./src/index.ts",
        "generate:docs": "ts-node src/utils/generate-docs.ts",
        "test": "NODE_ENV=test jest",
        "test:watch": "NODE_ENV=test jest --watch",
        "postinstall": "sh fix-client.sh"
    },
    "bin": {
        "mamoru-cli": "dist/index.js",
        "mamoru": "dist/index.js",
        "mamoru-cli-dev": "ts-node ./src/index.ts"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "@jest/globals": "^29.5.0",
        "@types/dashify": "^1.0.1",
        "@types/node": "^18.7.18",
        "@types/npm": "^7.19.0",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "babel-jest": "^29.5.0",
        "commander": "^9.4.0",
        "delay": "^5.0.0",
        "eslint": "^8.26.0",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^13.0.3",
        "npm": "^9.6.1",
        "prettier": "^2.7.1",
        "pretty-quick": "^3.1.3",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@cosmjs/json-rpc": "^0.29.5",
        "@cosmjs/tendermint-rpc": "^0.29.5",
        "@mamoru-ai/validation-chain-ts-client": "^0.0.3",
        "assemblyscript": "^0.27.1",
        "axios": "^0.27.2",
        "colors": "^1.4.0",
        "core-js": "3",
        "dashify": "^2.0.0",
        "handlebars": "^4.7.7",
        "ipfs-http-client": "56",
        "joi": "^17.6.0",
        "yaml": "^2.2.1"
    },
    "lint-staged": {
        "**/*.{js,ts}": [
            "npm run format",
            "npm run lint -- --fix"
        ]
    },
    "description": "## Installation",
    "bugs": {
        "url": "https://github.com/Mamoru-Foundation/mamoru-cli/issues"
    },
    "homepage": "https://github.com/Mamoru-Foundation/mamoru-cli#readme",
    "directories": {
        "doc": "docs"
    },
    "author": ""
}
